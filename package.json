{
  "name": "node-scrapy",
  "description": "Simple, lightweight and expressive web scraping with Node.js",
  "keywords": [
    "web",
    "html",
    "css",
    "selector",
    "scraping",
    "scrape",
    "scraper",
    "scrapy",
    "crawler"
  ],
  "version": "0.4.0-beta.1",
  "author": "Stefan Maric <me@stefanmaric.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/eeshi/node-scrapy.git"
  },
  "bugs": {
    "url": "https://github.com/eeshi/node-scrapy/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/eeshi/node-scrapy",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "dist/index.js",
  "files": [
    "browser/",
    "dist/",
    "lib/",
    "server/"
  ],
  "dependencies": {
    "cheerio": "https://github.com/kida7/cheerio.git",
    "lodash": "^4.17.15",
    "nearley": "^2.16.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/lodash": "^4.14.149",
    "@types/nearley": "^2.11.1"
  },
  "echint": {
    "ignore": [
      "coverage/**",
      "dist/**",
      ".nyc-output/**"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run grammar",
    "build": "",
    "pregrammar": "mkdir -p dist/",
    "grammar": "npx nearleyc src/query-parser.ne -o dist/grammar.js",
    "format": "prettier-eslint --write './**/*.{js,json,md}'",
    "pretest": "npm run grammar",
    "test": "echint && eslint . && tap 'test/**/*.test.js'",
    "prepublish": "npm run build"
  }
}