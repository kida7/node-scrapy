{
  "name": "node-scrapy",
  "description": "Simple, lightweight and expressive web scraping with Node.js",
  "keywords": [
    "web",
    "html",
    "css",
    "selector",
    "scraping",
    "scrape",
    "scraper",
    "scrapy",
    "crawler"
  ],
  "version": "0.4.0-beta.1",
  "author": "Stefan Maric <me@stefanmaric.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/eeshi/node-scrapy.git"
  },
  "bugs": {
    "url": "https://github.com/eeshi/node-scrapy/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/eeshi/node-scrapy",
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "dist/index.js",
  "files": [
    "browser/",
    "dist/",
    "lib/",
    "server/"
  ],
  "dependencies": {
    "cheerio": "https://github.com/kida7/cheerio.git",
    "htmlparser2": "*",
    "lodash": "*",
    "nearley": "*"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.16",
    "@types/lodash": "*",
    "@types/nearley": "*",
    "typescript": "*"
  },
  "echint": {
    "ignore": [
      "coverage/**",
      "dist/**",
      ".nyc-output/**"
    ]
  },
  "sideEffects": false,
  "scripts": {
    "build": "npx nearleyc src/query-parser.ne -o src/grammar.js && tsc"
  }
}
